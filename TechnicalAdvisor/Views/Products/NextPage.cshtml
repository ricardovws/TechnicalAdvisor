            <a href="#" class="list-group-item">
                @Html.DisplayFor(model => item.Title)

                <div class="list-group">

                    @foreach (var item_ in Model.Chapters)
                    {
                        <a href="#" class="list-group-item">
                            @Html.DisplayFor(model => item_.Title)

                        </a>
                    }

                </div>

            </a>
        }

    </div>
</div>






